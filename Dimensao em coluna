#include <iostream>
#include <ctime>
#include <cstdlib>


using namespace std;

int main(){
	int lin,col,i,j;
	
	
	cout <<"Informe a dimensao linha : ";
	cin>>lin;	
	
	cout <<"Informe a dimensao coluna : ";
	cin>>col;
	
	int mat[lin][col];
	srand(time(NULL));
	
	
	for(i = 0 ; i < lin; i++){
		for(j = 0; j< col; j++){
			mat[i][j] = rand()%10; //faixa de 0 a 9
		}
	}
	for(i = 0 ; i < lin; i++){
		for(j = 0; j< col; j++){
			cout <<"mat["<<i<<"]["<<j<<"]  = "<<mat[i][j]<<endl;
		}
		
}
return 0;
}
